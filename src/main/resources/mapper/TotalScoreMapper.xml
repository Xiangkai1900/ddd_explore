<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.infrastructure.datasource.TotalScoreDao">

    <insert id="saveTotalScore">
        insert into score_Total (student_number, total_score)
        value
        (#{studentNumber}, #{totalScore})
    </insert>


</mapper>
